{
  "name": "aie-plus",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "@loadable/babel-plugin": "^5.16.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.106",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "dayjs": "^1.11.13",
    "js-cookie": "^3.0.5",
    "nodemon": "^3.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "wait-on": "^8.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "build:test": "serve -s build",
    "lint": "eslint --quiet ./",
    "lint:fix": "eslint --fix . --ext .ts --ext .tsx",
    "lint:ci": "eslint -f json -o eslint-report.json ./",
    "prettier:format": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --silent=true",
    "ts:check": "tsc --noEmit",
    "dev": "concurrently -k \"BROWSER=none NODE_ENV=development npm start\" \"npm:electron\"",
    "electron:dev": "wait-on tcp:3000 && nodemon --exec electron .",
    "electron:build": "NODE_ENV=production npm run build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron": "wait-on tcp:3000 && electron .",
    "publish": "NODE_ENV=production npm run build && electron-builder -m -c.extraMetadata.main=build/electron.js --publish 'onTagOrDraft'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@coax/eslint-config-fe-react": "^1.0.0",
    "@craco/craco": "^7.1.0",
    "@types/js-cookie": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.7.0",
    "concurrently": "^8.2.2",
    "electron": "^32.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react": "^7.35.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.8",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.77.8",
    "sass-loader": "^13.3.3",
    "serve": "^14.2.3",
    "style-loader": "^3.3.4"
  },
  "pre-commit": [
    "ts:check",
    "prettier:format",
    "lint"
  ]
}
